<div>
    <h1>Desarrollo desafio </h1>
    
    <h3>Lista de Productos:</h3>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Titulo</th>
                <th>Descripcion</th>
                <th>Precio</th>
                <th>Imagen</th>
                <th>Codigo</th>
                <th>Stock</th>
                <th>Category</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody> 
            {{#each productos}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.thumbnail}}</td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
                <td>
                    {{#if this.status}}
                    Activo
                    {{else}}
                    Desactivado
                    {{/if}}
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>